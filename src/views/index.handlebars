<div>
  {{#each products}}
    <div class="card m-10">
      <div class="card-header">
        {{this.title}}
      </div>
      <div class="card-body">
        <p class="card-text">{{this.description}}</p>
        <p class="card-text">{{this.price}}</p>
        <p class="card-text">{{this.category}}</p>
        <p class="card-text">{{this.stock}}</p>
        <p class="card-text">{{this.code}}</p>
        <p class="card-text">{{this.id}}</p>
      </div>
    </div>
  {{/each}}
</div>